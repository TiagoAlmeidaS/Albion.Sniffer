{
  "RabbitMQ": {
    "ConnectionString": "amqp://myuser:mysecurepassword@tastechdeveloper.shop:5672",
    "Exchange": "albion.sniffer"
  },
  "Capture": {
    "Mode": "TCP",
    "UdpPort": 5050,
    "TcpEndpoint": "0.tcp.sa.ngrok.io:16179",
    "EnableFeatureFlag": true,
    "FallbackToUDP": true
  },
  "BinDumps": {
    "BasePath": "ao-bin-dumps",
    "Enabled": true,
    "AutoReload": false
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "AlbionOnlineSniffer": "Information",
      "AlbionOnlineSniffer.Capture": "Information",
      "AlbionOnlineSniffer.Core": "Information",
      "AlbionOnlineSniffer.Queue": "Information"
    },
    "Console": {
      "IncludeScopes": false,
      "TimestampFormat": "HH:mm:ss "
    }
  },
  "PacketMonitoring": {
    "EnableDetailedLogging": false,
    "EnableHexDump": false,
    "MetricsUpdateInterval": 30,
    "LogPeriodicStats": false,
    "AlertOnLowCaptureRate": false,
    "LowCaptureRateThreshold": 0.1
  }
}