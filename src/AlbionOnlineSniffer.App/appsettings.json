{
  "PacketCaptureSettings": {
    "InterfaceName": "", // Nome da interface de rede (ex: Ethernet, Wi-Fi)
    "Filter": "udp and port 5056", // Porta oficial do Albion Online
    "RequireDecryption": true, // Indica que os pacotes precisam ser descriptografados
    "CryptoniteIntegration": {
      "Enabled": true,
      "LocalPort": 5050, // Porta local onde o Cryptonite envia pacotes descriptografados
      "ExpectedSourcePort": 5056 // Porta original do Albion Online
    }
  },
  "QueueSettings": {
    "PublisherType": "RabbitMQ", // Ou "Redis"
    "RabbitMQ": {
      "HostName": "localhost",
      "QueueName": "albion_online_packets"
    },
    "Redis": {
      "ConnectionString": "localhost:6379",
      "ChannelName": "albion_online_packets"
    }
  },
  "BinDumps": {
    "BasePath": "ao-bin-dumps",
    "Enabled": true,
    "AutoReload": false
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "AlbionOnlineSniffer": "Debug"
    }
  }
}