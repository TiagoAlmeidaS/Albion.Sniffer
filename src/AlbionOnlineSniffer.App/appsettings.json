{
  "RabbitMQ": {
    "ConnectionString": "amqp://myuser:mysecurepassword@tastechdeveloper.shop:5672",
    "Exchange": "albion.sniffer"
  },
  "BinDumps": {
    "BasePath": "ao-bin-dumps",
    "Enabled": true,
    "AutoReload": false
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "AlbionOnlineSniffer": "Warning",
      "AlbionOnlineSniffer.Capture": "Warning",
      "AlbionOnlineSniffer.Core": "Warning",
      "AlbionOnlineSniffer.Queue": "Warning"
    },
    "Console": {
      "IncludeScopes": false,
      "TimestampFormat": "HH:mm:ss "
    }
  },
  "PacketMonitoring": {
    "EnableDetailedLogging": false,
    "EnableHexDump": false,
    "MetricsUpdateInterval": 30,
    "LogPeriodicStats": false,
    "AlertOnLowCaptureRate": false,
    "LowCaptureRateThreshold": 0.1
  }
}