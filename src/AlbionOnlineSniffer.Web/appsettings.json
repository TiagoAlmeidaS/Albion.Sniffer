{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "AlbionOnlineSniffer": "Information"
    }
  },
  "AllowedHosts": "*",
  "InMemoryRepositories": {
    "MaxPackets": 10000,
    "MaxEvents": 10000,
    "MaxLogs": 5000,
    "MaxSessions": 1000
  },
  "Capture": {
    "Mode": "UDP",
    "UdpPort": 5050,
    "TcpEndpoint": "0.tcp.sa.ngrok.io:16179",
    "EnableFeatureFlag": false,
    "FallbackToUDP": true
  },
  "RabbitMQ": {
    "ConnectionString": "amqp://myuser:mysecurepassword@tastechdeveloper.shop:5672",
    "Exchange": "albion.sniffer"
  },
  "BinDumps": {
    "BasePath": "ao-bin-dumps",
    "Enabled": true,
    "AutoReload": false
  },
  "HealthChecks": {
    "Enabled": true,
    "Interval": "00:00:30",
    "Timeout": "00:00:10"
  },
  "Metrics": {
    "Enabled": true,
    "PrometheusEndpoint": "/metrics",
    "RetentionPeriod": "01:00:00"
  }
}