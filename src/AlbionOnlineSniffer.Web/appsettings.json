{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "InMemoryRepositories": {
    "MaxPackets": 10000,
    "MaxEvents": 10000,
    "MaxLogs": 5000,
    "MaxSessions": 1000
  },
  "BinDumps": {
    "Enabled": true,
    "BasePath": "ao-bin-dumps"
  },
  "RabbitMQ": {
    "ConnectionString": "amqp://myuser:mysecurepassword@tastechdeveloper.shop:5672",
    "Exchange": "albion.sniffer"
  },
  "HealthChecks": {
    "Enabled": true,
    "Interval": "00:00:30",
    "Timeout": "00:00:10"
  },
  "Metrics": {
    "Enabled": true,
    "PrometheusEndpoint": "/metrics",
    "RetentionPeriod": "01:00:00"
  }
}