{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "InMemoryRepositories": {
    "MaxPackets": 10000,
    "MaxEvents": 10000,
    "MaxLogs": 5000,
    "MaxSessions": 1000
  },
  "Queue": {
    "Type": "RabbitMQ",
    "ConnectionString": "amqp://localhost:5672",
    "QueueName": "albion-sniffer",
    "ExchangeName": "albion-sniffer",
    "RoutingKey": "packets"
  },
  "BinDumps": {
    "Enabled": true,
    "BasePath": "ao-bin-dumps"
  },
  "HealthChecks": {
    "Enabled": true,
    "Interval": "00:00:30",
    "Timeout": "00:00:10"
  },
  "Metrics": {
    "Enabled": true,
    "PrometheusEndpoint": "/metrics",
    "RetentionPeriod": "01:00:00"
  }
}